        <span class="err">\</span><span class="n">coordinate</span> <span class="p">[</span><span class="n">label</span><span class="o">=</span><span class="n">below</span><span class="o">:</span><span class="err">$</span><span class="n">E</span><span class="err">$</span><span class="p">]</span> <span class="p">(</span><span class="n">E</span><span class="p">)</span> <span class="n">at</span> <span class="p">(</span><span class="mi">28</span><span class="p">,</span><span class="mi">0</span><span class="p">);</span>
