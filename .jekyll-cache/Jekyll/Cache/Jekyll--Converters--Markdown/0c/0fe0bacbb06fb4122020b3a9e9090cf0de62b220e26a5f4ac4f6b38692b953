        <span class="err">\</span><span class="n">path</span> <span class="p">[</span><span class="n">name</span> <span class="n">intersections</span><span class="o">=</span><span class="p">{</span><span class="n">of</span><span class="o">=</span><span class="n">UVW</span> <span class="n">and</span> <span class="n">GH</span><span class="p">}]</span>
            <span class="n">coordinate</span> <span class="p">[</span><span class="n">label</span><span class="o">=</span><span class="n">above</span> <span class="n">left</span><span class="o">:</span><span class="err">$</span><span class="n">W</span><span class="err">$</span><span class="p">]</span> <span class="p">(</span><span class="n">W</span><span class="p">)</span> <span class="n">at</span> <span class="p">(</span><span class="n">intersection</span><span class="o">-</span><span class="mi">1</span><span class="p">);</span>
        <span class="err">\</span><span class="n">draw</span> <span class="p">[</span><span class="n">very</span> <span class="n">thick</span><span class="p">]</span> <span class="p">(</span><span class="n">W</span><span class="p">)</span> <span class="o">--</span> <span class="p">(</span><span class="n">Q</span><span class="p">);</span>

        <span class="o">%</span> <span class="err">绘制镰刀把</span>
        <span class="err">\</span><span class="n">coordinate</span> <span class="p">[</span><span class="n">label</span><span class="o">=</span><span class="n">below</span><span class="o">:</span><span class="err">$</span><span class="n">X</span><span class="err">$</span><span class="p">]</span> <span class="p">(</span><span class="n">X</span><span class="p">)</span> <span class="n">at</span> <span class="p">(</span><span class="mi">2</span><span class="p">.</span><span class="mi">5</span><span class="p">,</span><span class="mi">2</span><span class="p">.</span><span class="mi">5</span><span class="p">);</span>
        <span class="err">\</span><span class="n">coordinate</span> <span class="p">[</span><span class="n">label</span><span class="o">=</span><span class="n">below</span> <span class="n">right</span><span class="o">:</span><span class="err">$</span><span class="n">Y</span><span class="err">$</span><span class="p">]</span> <span class="p">(</span><span class="n">Y</span><span class="p">)</span> <span class="n">at</span> <span class="p">(</span><span class="mi">5</span><span class="p">,</span><span class="mi">3</span><span class="p">);</span>
